{
    "name": "vendor-name/request-shield",
    "description": "Laravel package to protect against malicious IPs and User-Agents",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "VendorName\\RequestShield\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VendorName\\RequestShield\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VendorName\\RequestShield\\ShieldServiceProvider"
            ],
            "facades": [
                "VendorName\\RequestShield\\Facades\\Shield"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
