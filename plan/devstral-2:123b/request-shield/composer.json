{
    "name": "vendorname/request-shield",
    "description": "Laravel package to block malicious bots and IPs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "VendorName\\RequestShield\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VendorName\\RequestShield\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VendorName\\RequestShield\\RequestShieldServiceProvider"
            ],
            "aliases": {
                "Shield": "VendorName\\RequestShield\\Facades\\ShieldFacade"
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ]
    }
}