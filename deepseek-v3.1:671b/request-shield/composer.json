{
    "name": "vendorname/request-shield",
    "description": "Laravel middleware to block malicious bots and specific IPs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your@email.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/console": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "VendorName\\RequestShield\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VendorName\\RequestShield\\RequestShieldServiceProvider"
            ],
            "aliases": {
                "Shield": "VendorName\\RequestShield\\Facades\\Shield"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}