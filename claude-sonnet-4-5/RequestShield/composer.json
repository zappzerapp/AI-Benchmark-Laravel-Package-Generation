{
    "name": "vendorname/request-shield",
    "description": "A Laravel middleware package to block malicious bots and specific IPs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "VendorName",
            "email": "info@vendorname.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "VendorName\\RequestShield\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VendorName\\RequestShield\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VendorName\\RequestShield\\RequestShieldServiceProvider"
            ],
            "aliases": {
                "Shield": "VendorName\\RequestShield\\Facades\\Shield"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
