{
    "name": "vendor-name/request-shield",
    "description": "Laravel package to block malicious bots and specific IPs via middleware",
    "type": "laravel-package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "security",
        "middleware",
        "ip-blocker",
        "bot-protection"
    ],
    "homepage": "https://github.com/vendor-name/request-shield",
    "authors": [
        {
            "name": "VendorName",
            "email": "developer@vendor-name.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "VendorName\\RequestShield\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VendorName\\RequestShield\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VendorName\\RequestShield\\RequestShieldServiceProvider"
            ],
            "facades": [
                "VendorName\\RequestShield\\Facades\\Shield"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
